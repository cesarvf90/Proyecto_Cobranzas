@model IEnumerable<RecaudaSoft.Models.Acreedor>

@{
    ViewBag.Title = "Index";
}

<h2>Acreedores</h2>

<p>
    @Html.ActionLink("Nuevo acreedor", "Create")
</p>
<table>
    <tr>
        <th>
            idAcreedor
        </th>
        <th>
            nombre
        </th>
        <th>
            razonSocial
        </th>
        <th>
            ruc
        </th>
        <th>
            direccion
        </th>
        <th>
            rubro
        </th>
        <th>
            telefono
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idAcreedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.razonSocial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ruc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.rubro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.telefono)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idAcreedor }) |
            @Html.ActionLink("Details", "Details", new { id = item.idAcreedor }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.idAcreedor })
        </td>
    </tr>
}

</table>
