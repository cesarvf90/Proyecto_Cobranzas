@model RecaudaSoft.ViewModels.ModeloAsignacion

@{
    ViewBag.Title = "AsignarTareas";
}

<h2>La asignación de tareas realizada fue la siguiente:</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            idGestorXDeuda
        </th>
        <th>
            idGestor
        </th>
        <th>
            idDeuda
        </th>
        <th>
            fechaAsignacion
        </th>
        <th>
            exito
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.gestoresXdeudas) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idGestorXDeuda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idGestor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idDeuda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaAsignacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.exito)
        </td>
    </tr>
}

</table>
